<Window x:Class="CSV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSV"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnSelectFile" Content="Select" HorizontalAlignment="Left" Margin="522,60,0,0" VerticalAlignment="Top" Width="80" Height="37" Click="btnSelectFile_Click"/>
        <Label Content="Enter File Path:" HorizontalAlignment="Left" Margin="23,65,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtFilePath" HorizontalAlignment="Left" Height="23" Margin="116,69,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="378"/>
        <ListBox x:Name="lstLines" Margin="10,195,10,10">
            <ListBoxItem Content="1"/>
        </ListBox>
        <Button x:Name="btnProcess" Content="Process" HorizontalAlignment="Left" Margin="266,133,0,0" VerticalAlignment="Top" Width="117" Height="57" IsEnabled="False" Click="btnProcess_Click"/>

    </Grid>
</Window>
